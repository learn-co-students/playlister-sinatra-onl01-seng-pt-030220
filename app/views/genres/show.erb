<h1><%=@genre.name%></h1>



<div><h3>Genre's songs:  </h3>
<%@genre.songs.each do |song|%>

<a href="/songs/<%= song.slug %>"><%= song.name %></a><br>

<%end%>

</div>




<div>
<h3>Genre's artists : </h3>
<%@genre.artists.each do |artist|%>
<a href="/artists/<%= artist.slug %>"><%= artist.name %><br>
<%end%>
</div>
<br>
