<h1><%=@artist.name%></h1>

<div>
<h3>Artists songs : </h3>
<%@artist.songs.each do |song|%>
<a href="/songs/<%= song.slug %>"><%= song.name %><br>
<%end%>
</div>
<br>


<div><h3>Artists Genres:  </h3>
<%@artist.genres.each do |genre|%>

<a href="/genres/<%= genre.slug %>"><%= genre.name %></a><br>

<%end%>

</div>
